package org.simplilearn.workshop.model;

public class Customer {
	
	private Long id;
	private String name;
	private String email;
	private String phone;
	
	public Customer() {
		super();
	}
	
	public Customer(Long id , String name, String email , String phone) {
		this.id = id;
		this.name = name;
		this.email = email;
		this.phone = phone;
	}

}
